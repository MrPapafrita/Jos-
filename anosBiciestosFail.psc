Algoritmo Años
	Escribir "Ingresar edad";
	Leer añosVividos;
	añosBiciestos<- añosVividos/4
	biciestosCant<- 366
	añosNormales<- añosVividos-añosBiciestos
	normalesCant<- 365
	diasVividos<- (añosViciestos*biciestosCant)+(añosNormales*normalesCant)
	Escribir  "Los dias vividos son", diasVividos;
	
FinAlgoritmo
